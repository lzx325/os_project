package main

import (
	"fmt"
	"time"
)

// import "time"

type A struct {
	a int
	b bool
}

func fun(obj *A) {
	obj.a = 2
}
func compare(a, b time.Duration) bool {
	return a < b
}
func main() {
	ch := make(chan int)
	go func() {
		time.Sleep(4 * time.Second)
		ch <- 1
	}()
	timer := time.After(2 * time.Second)
	select {
	case <-ch:
		fmt.Println("case 1")
	case <-timer:
		fmt.Println("case 2")
	}
}
